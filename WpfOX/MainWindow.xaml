<Window x:Class="WpfOX.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfOX"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button x:Name="Position1" Height="60" Width="120" Grid.Column="1" Grid.Row="1" Click="Position1_Click">X</Button>
        <Button x:Name="Position2" Height="60" Width="120" Grid.Column="2" Grid.Row="1" Click="Position2_Click">X</Button>
        <Button x:Name="Position3" Height="60" Width="120" Grid.Column="3" Grid.Row="1" Click="Position3_Click">X</Button>
        <Button x:Name="Position4" Height="60" Width="120" Grid.Column="1" Grid.Row="2" Click="Position4_Click">X</Button>
        <Button x:Name="Position5" Height="60" Width="120" Grid.Column="2" Grid.Row="2" Click="Position5_Click">X</Button>
        <Button x:Name="Position6" Height="60" Width="120" Grid.Column="3" Grid.Row="2" Click="Position6_Click">X</Button>
        <Button x:Name="Position7" Height="60" Width="120" Grid.Column="1" Grid.Row="3" Click="Position7_Click">X</Button>
        <Button x:Name="Position8" Height="60" Width="120" Grid.Column="2" Grid.Row="3" Click="Position8_Click">X</Button>
        <Button x:Name="Position9" Height="60" Width="120" Grid.Column="3" Grid.Row="3" Click="Position9_Click">X</Button>
    </Grid>
</Window>
